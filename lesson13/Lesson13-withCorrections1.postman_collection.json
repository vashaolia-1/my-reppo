		{
			"name": "Get votes, check added vote to image",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"pm.test(\"Check if vote has proper vote Id\", function() {",
							"    const json = pm.response.json();",
							"    const voteId = pm.collectionVariables.get(\"voteId\");",
							"    pm.expect(voteId, \"voteId collection variable should be defined\").to.not.be.undefined;",
							"    const voteObj = json.find(obj => obj.id == voteId);",
							"    pm.expect(voteObj, `Vote object with id ${voteId} should exist in response`).to.exist;",
							"    if (voteObj) {",
							"        pm.collectionVariables.set(\"voteId\", voteObj.id);",
							"        pm.collectionVariables.set(\"user_id\", voteObj.user_id);",
							"    }",
							"});",
							"pm.test(\"Check if vote has proper image object with id and url\", function () {",
							"    const json = pm.response.json();",
							"    const voteId = pm.collectionVariables.get(\"voteId\");",
							"    const voteObj = json.find(obj => obj.id == voteId);",
							"    pm.expect(voteObj).to.have.property(\"image\");",
							"    if (voteObj && voteObj.image) {",
							"        pm.expect(voteObj.image).to.have.property(\"id\");",
							"        pm.expect(voteObj.image).to.have.property(\"url\");",
							"        pm.expect(voteObj.image.id, \"image.id should not be empty\").to.not.be.empty;",
							"        pm.expect(voteObj.image.url, \"image.url should not be empty\").to.not.be.empty;",
							"        pm.expect(voteObj.image.id, \"image.id should match image_id property of vote object\").to.eql(voteObj.image_id);",
							"    }",
							"});"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
